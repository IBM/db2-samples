SQL_DROP_TABLE=DROP TABLE IF EXISTS embeddings
SQL_CREATE_TABLE=CREATE TABLE embeddings (id INT NOT NULL GENERATED ALWAYS AS IDENTITY (START WITH 1, INCREMENT BY 1), content CLOB, source VARCHAR(255), title VARCHAR(255), embedding VECTOR(1024, FLOAT32), PRIMARY KEY (id))
SQL_INSERT=INSERT INTO embeddings(content, source, title, embedding) VALUES (?, ?, ?, VECTOR('[{embedding_vector_str}]', 1024, FLOAT32))
SQL_DISTANCE=SELECT content as CONTENT, source as SOURCE, title as TITLE, VECTOR_DISTANCE(VECTOR('[{query_embedding_str}]', 1024, FLOAT32), embedding, EUCLIDEAN) AS DISTANCE FROM embeddings ORDER BY DISTANCE ASC FETCH FIRST {top_k} ROWS ONLY